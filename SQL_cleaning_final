and country is null
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'United Kingdom of Great Britain and Northern Ireland'
WHERE (geoname LIKE '%England%'and (geoname NOT LIKE "% and %"
and geoname not like "%church of england%"
and geoname not like "%battle%"
and geoname not like "%neuengland%"
and geoname not like "%New England%"));

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Scotland%'
ORDER BY geoname;

