ORDER BY geoname;

UPDATE sixfiveone
SET country = 'United Kingdom of Great Britain and Northern Ireland'
WHERE (geoname LIKE '%Northern Ireland%'and (geoname NOT LIKE "% and %"
AND geoname NOT LIKE "%in literature%"));

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Great Britain%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'United Kingdom of Great Britain and Northern Ireland'
WHERE (geoname LIKE '%Great Britain%'and (geoname NOT LIKE "% and %"
AND geoname NOT LIKE "%in literature%"
AND geoname NOT LIKE "%in art%"
AND geoname NOT LIKE "%Hong Kong%"
AND geoname NOT LIKE "%foreign relations%"
AND geoname NOT LIKE "%in literature%"
AND geoname NOT IN ("Protestants in Great Britain.","Great Britain Royal Navy","Jews, Russian, in Great Britain","Great Britain History Edward V","Great Britain Miscellaneous Island Dependencies")));

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Wales%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'United Kingdom of Great Britain and Northern Ireland'
WHERE (geoname LIKE '%Wales%'and (geoname NOT LIKE "% and %"
AND geoname NOT LIKE "%New South Wales%"
AND geoname NOT LIKE "%Prince of Wale%"
AND geoname NOT LIKE "%Alaska%"
AND geoname NOT LIKE "%(Mass.%"));

