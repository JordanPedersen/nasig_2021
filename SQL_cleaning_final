SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%United Arab Emirates%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'United Arab Emirates'
WHERE (geoname LIKE '%United Arab Emirates%');

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Afghanistan%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'Afghanistan'
WHERE (geoname LIKE '%Afghanistan%'
AND geoname NOT LIKE '% and %');

