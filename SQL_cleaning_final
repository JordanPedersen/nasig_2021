SELECT * from sixfiveone
WHERE geoname LIKE "% and %"
AND country IS NULL;

UPDATE sixfiveone
SET country = 'Multiple_Countries'
WHERE geoname LIKE "% and %"
AND country IS NULL;

SELECT * from sixfiveone
WHERE geoname LIKE "%U.S.S.R%"
OR geoname LIKE "%Czechoslovakia%"
OR geoname LIKE "%Extinct%"
OR geoname LIKE "%Ancient%"
AND country IS NULL;

UPDATE sixfiveone
SET country = 'Historical_Place'
WHERE geoname LIKE "%U.S.S.R%"
OR geoname LIKE "%Czechoslovakia%"
OR geoname LIKE "%Extinct%"
OR geoname LIKE "%Ancient%"
OR (geoname LIKE '%Germany%'
AND country IS NULL
AND (geoname LIKE "%West%"
OR geoname LIKE "%East%"))
AND country IS NULL;

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Palestine%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'Palestine, State of'
WHERE (geoname LIKE '%Palestine%'
AND geoname NOT LIKE "% and %"
AND geoname NOT LIKE "Palestine in %"
AND geoname NOT IN ("Palestine (Tex.)"));

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Sao Tome%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'Sao Tome and Principe'
WHERE (geoname LIKE '%Sao Tome and Principe%');

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%South Sudan%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'South Sudan'
WHERE (geoname LIKE '%South Sudan%'and (geoname NOT LIKE "% and %"));

