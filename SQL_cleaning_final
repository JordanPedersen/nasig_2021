AND geoname NOT LIKE "%Spainmala%"
AND geoname NOT LIKE "%battle%"
AND geoname NOT LIKE "%Extinct%"));

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%China%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'China'
WHERE (geoname LIKE "%(China)%");

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%China%'
AND country IS NULL
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'China'
WHERE (geoname LIKE "%, China)%");

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%China%'
AND country IS NULL
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'China'
WHERE (geoname LIKE "%China%"
AND geoname NOT LIKE "%Chinatown%"
AND geoname NOT LIKE "%Indo-China%"
AND geoname NOT LIKE "%Indochina%"
AND geoname NOT LIKE "%National gov%"
AND geoname NOT LIKE "%in art%"
AND geoname NOT LIKE "%in literature%"
AND geoname NOT LIKE "%and%"
AND geoname NOT LIKE "%government%"
AND geoname NOT LIKE "%soviet%"
AND geoname NOT LIKE "%former%"
AND geoname NOT LIKE "%history%"
AND geoname NOT LIKE "%china sea%"
AND geoname NOT LIKE "%economic%"
AND geoname NOT LIKE "%motion pictures%"
AND geoname NOT LIKE "%foreign relations%"
AND geoname NOT LIKE "%fiction%"
AND geoname NOT LIKE "%investiments%"
AND geoname NOT LIKE "%consultants%");

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Equatorial Guinea%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'Equatorial Guinea'
WHERE (geoname LIKE '%Equatorial Guinea%');

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Papua New Guinea%'
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'Papua New Guinea'
WHERE (geoname LIKE '%Papua New Guinea%'
AND geoname NOT LIKE "%literature%"
AND geoname NOT LIKE "%region%"
AND geoname NOT LIKE "% and %");

SELECT DISTINCT geoname FROM sixfiveone WHERE geoname LIKE '%Guinea%'
AND country IS NULL
ORDER BY geoname;

UPDATE sixfiveone
SET country = 'Guinea'
WHERE (geoname LIKE '%Guinea%'
AND country IS NULL)
AND geoname NOT LIKE "%Spanish%"
AND geoname NOT LIKE "%Dutch%"
AND geoname NOT LIKE "%Gulf%"
AND geoname NOT LIKE "%New Guinea%"
AND geoname NOT LIKE "%Papua%"
AND geoname NOT LIKE "% and %";

UPDATE sixfiveone
SET country = 'Guinea-Bissau'
WHERE (geoname = 'Guinea Bissau');

UPDATE sixfiveone
SET country = 'Papua New Guinea'
WHERE (geoname = 'Bougainville Island Region (Papua New Guinea)');





